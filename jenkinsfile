pipeline{
	agent any
	
		stages{
			stage('Compile Stage'){
				steps{
					withMaven(maven : 'Maven'){
						sh 'mvn clean compile'
					}
				}
			}
			stage('Unit Testing Stage'){
				steps{
					withMaven(maven : 'Maven'){
						sh 'mvn clean test jacoco:report'
					}
				}
			}
			stage('Stage3'){
				steps{
					echo 'This is Stage 3'
				}
			}
		}
}  
